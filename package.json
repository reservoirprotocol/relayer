{
  "name": "@reservoir/opensea-indexer",
  "version": "0.0.1",
  "description": "Service for continuously monitoring/scraping Opensea data",
  "repository": "https://github.com/reservoirprotocol/opensea-indexer",
  "author": "Reservoir Team",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist tsconfig.tsbuildinfo",
    "compile": "tsc",
    "build": "yarn migrate up && yarn clean && yarn compile",
