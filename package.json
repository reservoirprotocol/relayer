    "migrate": "ts-node ./node_modules/node-pg-migrate/bin/node-pg-migrate -m ./src/migrations"
  },
  "devDependencies": {
    "@georgeroman/wyvern-v2-sdk": "0.0.27",
    "@types/node": "^16.10.1",
    "@types/node-cron": "^2.0.4",
    "axios": "^0.21.4",
    "dotenv": "^10.0.0",
    "node-cron": "^3.0.0",
    "node-pg-migrate": "^6.0.0",
    "pg": "^8.7.1",
    "pg-promise": "^10.11.0",
    "tedis": "^0.1.12",
    "throttled-queue": "^1.0.7",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.3"
  }
}